// https://codeforces.com/problemset/problem/1993/A

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef double dl;

#define endl "\n"
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define fraction() cout.unsetf(ios::floatfield); cout.precision(10); cout.setf(ios::fixed,ios::floatfield);

int main(){

   optimize ();

int t; cin>>t;
while(t--){

int n; cin>>n;
 int a=0,b=0,c=0,d=0,ans=0;
 string s; cin>>s;

   for(int i=0;i<s.size();i++){
     if(s[i]=='A') a++;
     else if(s[i]=='B') b++;
     else if(s[i]=='C') c++;
     else if(s[i]=='D') d++;

     }

       ans=min(a,n)+min(b,n)+min(c,n)+min(d,n);
       cout<<ans<<endl;

}


return 0;

}
